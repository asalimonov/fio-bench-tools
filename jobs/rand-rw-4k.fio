; rand-rw-4k.job for fiotest

[global]
name=fio-rand-rw-4k
filename=fio-rand-rw-4k
rw=randrw
rwmixread=60
rwmixwrite=40
bs=4K
direct=0
numjobs=4
time_based=1
runtime=900

[rand-file1]
size=10G
ioengine=libaio
iodepth=1

[rand-file2]
size=10G
ioengine=libaio
iodepth=2

[rand-file4]
size=10G
ioengine=libaio
iodepth=4

[rand-file8]
size=10G
ioengine=libaio
iodepth=8

[rand-file16]
size=10G
ioengine=libaio
iodepth=16

[rand-file32]
size=10G
ioengine=libaio
iodepth=32
